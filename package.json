{
	"name": "vscode-string-converter",
	"displayName": "VSCode String Converter",
	"description": "This extension converts the selected strings to a specific format",
	"author": {
		"name": "Bruno Vasconcelos",
		"email": "brunohgv2@gmail.com",
		"url": "https://github.com/brunohgv"
	},
	"publisher": "brunohgv",
	"homepage": "https://github.com/brunohgv/vs-code-string-converter",
	"repository": {
		"url": "https://github.com/brunohgv/vs-code-string-converter"
	},
	"icon": "images/string_converter_logo.png",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.camelToKebab",
		"onCommand:extension.camelToSnake",
		"onCommand:extension.camelToTitle",
		"onCommand:extension.kebabToCamel",
		"onCommand:extension.kebabToSnake",
		"onCommand:extension.kebabToTitle",
		"onCommand:extension.titleToCamel",
		"onCommand:extension.titleToSnake",
		"onCommand:extension.titleToKebab",
		"onCommand:extension.toLower",
		"onCommand:extension.toUpper"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.camelToKebab",
				"title": "Camel To Kebab"
			},
			{
				"command": "extension.camelToSnake",
				"title": "Camel To Snake"
			},
			{
				"command": "extension.camelToTitle",
				"title": "Camel To Title"
			},
			{
				"command": "extension.kebabToCamel",
				"title": "Kebab To Camel"
			},
			{
				"command": "extension.kebabToSnake",
				"title": "Kebab To Snake"
			},
			{
				"command": "extension.kebabToTitle",
				"title": "Kebab To Title"
			},
			{
				"command": "extension.titleToCamel",
				"title": "Title to Camel"
			},
			{
				"command": "extension.titleToSnake",
				"title": "Title to Snake"
			},
			{
				"command": "extension.titleToKebab",
				"title": "Title to Kebab"
			},
			{
				"command": "extension.snakeToCamel",
				"title": "Snake to Camel"
			},
			{
				"command": "extension.snakeToTitle",
				"title": "Snake to Title"
			},
			{
				"command": "extension.snakeToKebab",
				"title": "Snake to Kebab"
			},
			{
				"command": "extension.toLower",
				"title": "To Lower Case"
			},
			{
				"command": "extension.toUpper",
				"title": "To Upper Case"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^8.43.0",
		"glob": "^7.1.5",
		"mocha": "^10.1.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
